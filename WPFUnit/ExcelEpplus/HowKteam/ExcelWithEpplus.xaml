<Window x:Class="WPFUnit.ExcelEpplus.ExcelWithEpplus"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFUnit.ExcelEpplus"
        mc:Ignorable="d"
        Title="ExcelWithEpplus" Height="450" Width="800">
    <StackPanel>
        <DataGrid Height="200" x:Name="dtgExcel"></DataGrid>
        <Button x:Name="btnCreate" Click="btnCreate_Click" Content="Create"></Button>
        <Button x:Name="btnImport" Click="btnImport_Click" Content="Import"></Button>
        <Button x:Name="btnExport" Click="btnExport_Click" Content="Export"></Button>
    </StackPanel>
</Window>
